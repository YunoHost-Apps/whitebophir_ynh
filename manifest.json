{
    "name": "Whitebophir",
    "id": "whitebophir",
    "packaging_format": 1,
    "description": {
        "en": "Open source vector collaborative board",
        "fr": "Tableau de dessin vectoriel collaboratif open source"
    },
    "url": "https://wbo.ophir.dev/",
    "version": "1.9.1~ynh2",
    "license": "AGPL-3.0-only",
    "maintainer": [{
        "name": "bwjtkw",
        "email": "baptiste@wojtkowski.fr"
    },
    {
        "name": "oiseauroch.",
        "email": "tobias.ollive@oiseauroch.fr"
    }],
    "requirements": {
        "yunohost": ">= 4.1.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "domain.org"
            },
            {
                "name": "path",
                "type": "path",
                "example": "wbo",
		"default": "/"
            },
            {
                "name": "is_public",
                "type": "boolean",
		"help": {
		    "en" : "This application will be accessible publiquely. Anyone will be able to create boards.",
		    "fr" : "Cette application sera accessible publiquement, n'importe qui pourra cr√©er des tableaux"
		},
                "default": true
            }
        ]
    }
}
